<% @page_description = t(:contact_description) -%>
<% @page_title = @page.title -%>

<div id="staticContent" class="clearfix">
  <h1><%= @page.title %></h1>
  <% unless @page.caption.blank? %>
  <em><%= raw @page.caption %></em>
  <% end %>
  <%= raw @page.body %>
  <%= render :partial => 'contact_form', :locals => { :comment => @comment, :ip => @ip } %>
</div> <!-- end browser --><!-- end featured --> 
