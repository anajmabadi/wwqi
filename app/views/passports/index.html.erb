<h1>Listing passports</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Owner</th>
    <th>Item</th>
    <th>Publish</th>
    <th>Primary</th>
    <th>Position</th>
    <th>Notes</th>
    <th>Custom url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @passports.each do |passport| %>
  <tr>
    <td><%= passport.tag %></td>
    <td><%= passport.owner_id %></td>
    <td><%= passport.item_id %></td>
    <td><%= passport.publish %></td>
    <td><%= passport.primary %></td>
    <td><%= passport.position %></td>
    <td><%= passport.notes %></td>
    <td><%= passport.custom_url %></td>
    <td><%= link_to 'Show', passport %></td>
    <td><%= link_to 'Edit', edit_passport_path(passport) %></td>
    <td><%= link_to 'Destroy', passport, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Passport', new_passport_path %>
