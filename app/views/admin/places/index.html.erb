<h1>Listing places</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>X</th>
    <th>Y</th>
    <th>Publish</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.name %></td>
    <td><%= place.latitude %></td>
    <td><%= place.longitude %></td>
    <td><%= place.x %></td>
    <td><%= place.y %></td>
    <td><%= place.publish %></td>
    <td><%= place.notes %></td>
    <td><%= link_to 'Show', admin_place_path(place) %></td>
    <td><%= link_to 'Edit', edit_admin_place_path(place) %></td>
    <td><%= link_to 'Destroy', admin_place_path(place), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Place', new_admin_place_path %>
