<% form_for(@categorization) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :category_id %><br />
    <%= f.select :category_id, @categories %>
  </div>
  <div class="field">
    <%= f.label :item_id %><br />
    <%= f.text_field :item_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
